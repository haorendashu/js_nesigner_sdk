{
  "name": "js_nesigner_sdk",
  "version": "0.1.3",
  "description": "A JavaScript SDK for the nesigner.",
  "type": "module",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.es.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs.js"
      }
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --outDir dist",
    "dev": "vite"
  },
  "keywords": [
    "nesigner",
    "sdk",
    "nostr",
    "hardware",
    "wallet"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0"
  },
  "dependencies": {
    "@noble/ciphers": "^1.3.0",
    "@noble/curves": "^1.9.2",
    "@noble/hashes": "^1.8.0",
    "@scure/base": "^1.2.6",
    "@types/w3c-web-serial": "^1.0.8",
    "ts-md5": "^1.3.1"
  }
}